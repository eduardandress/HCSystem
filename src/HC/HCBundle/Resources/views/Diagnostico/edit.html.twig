{% extends 'HCHCBundle::layout.html.twig' %}


{% block content_header %}
  <section class="content-header">
    <h1>
      Editar Diagnostico
      <small>Editar el Diagnostico seleccionado</small>
    </h1>
    <ol class="breadcrumb">
      <li><a href="#"><i class="fa fa-dashboard"></i> Diagnostico</a></li>
      <li class="active">aqui</li>
    </ol>
  </section>

{% endblock %}


{% block content -%}
 <div class="col-md-6 ">
    {{ form_start(edit_form) }}
    {{ form_row(edit_form._token) }}
    {{ form_row(edit_form.descripcion,{'attr':{'class':'form-control'}}) }}
    {{ form_row(edit_form.idnotacita) }}
    {{ form_row(edit_form.idusuario) }}

    <center>
      <div class="btn-group">
              {{ form_end(edit_form) }}
              <a href="{{app.request.headers.get('referer')}}" class="btn btn-default">
              Regresar
              </a>
            {{ form(delete_form) }}
      </div>
    </center>  
{% endblock %}