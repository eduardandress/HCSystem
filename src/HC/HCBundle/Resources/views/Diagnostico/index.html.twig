{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Diagnostico list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Iddiagnostico</th>
                <th>Fecha</th>
                <th>Descripcion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('diagnostico_show', { 'id': entity.iddiagnostico }) }}">{{ entity.iddiagnostico }}</a></td>
                <td>{% if entity.fecha %}{{ entity.fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.descripcion }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('diagnostico_show', { 'id': entity.iddiagnostico }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('diagnostico_edit', { 'id': entity.iddiagnostico }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('diagnostico_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
