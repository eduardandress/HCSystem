{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Pacnumcontacto list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idpacnumcontacto</th>
                <th>Nombrecontacto</th>
                <th>Apellidocontacto</th>
                <th>Numero</th>
                <th>Relacion</th>
                <th>Fechaagregado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('pacnumcontacto_show', { 'id': entity.idpacnumcontacto }) }}">{{ entity.idpacnumcontacto }}</a></td>
                <td>{{ entity.nombrecontacto }}</td>
                <td>{{ entity.apellidocontacto }}</td>
                <td>{{ entity.numero }}</td>
                <td>{{ entity.relacion }}</td>
                <td>{% if entity.fechaagregado %}{{ entity.fechaagregado|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('pacnumcontacto_show', { 'id': entity.idpacnumcontacto }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('pacnumcontacto_edit', { 'id': entity.idpacnumcontacto }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('pacnumcontacto_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
