{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Phcicondicion list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Idphcicondicion</th>
                <th>Descripcion</th>
                <th>Fechaactualizacion</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('phcicondicion_show', { 'id': entity.idphcicondicion }) }}">{{ entity.idphcicondicion }}</a></td>
                <td>{{ entity.descripcion }}</td>
                <td>{% if entity.fechaactualizacion %}{{ entity.fechaactualizacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('phcicondicion_show', { 'id': entity.idphcicondicion }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('phcicondicion_edit', { 'id': entity.idphcicondicion }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('phcicondicion_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
